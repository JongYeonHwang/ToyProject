<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:ignore="MissingDefaultResource">

    <View
            android:background="#D6F2F3"
            android:layout_width="413dp"
            android:layout_height="739dp" android:id="@+id/view7" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <View
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/view5"
            app:layout_constraintTop_toBottomOf="@+id/view4" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.647" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <View
            android:background="@color/projectBlue4"
            android:layout_width="414dp"
            android:layout_height="56dp" android:id="@+id/view4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
    />
    <androidx.recyclerview.widget.RecyclerView
            android:padding="20dp"
            android:background="@drawable/view_shadow"
            android:layout_width="330dp"
            android:layout_height="526dp"
            android:id="@+id/recyclerView"
            app:layout_constraintTop_toTopOf="@+id/view4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.672" app:layout_constraintHorizontal_bias="0.493"/>

    <TextView
            android:textColor="@color/white"
            android:textStyle="bold"
            android:text="FASHION"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            app:layout_constraintTop_toTopOf="@+id/view4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="14dp"
            android:textSize="24sp" app:layout_constraintBottom_toTopOf="@+id/view5"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageButton
            android:scaleType="fitXY"
            android:background="#00ff0000"
            android:layout_width="24dp"
            android:layout_height="18dp" app:srcCompat="@drawable/arrow" android:id="@+id/homebtn"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="30dp"
            app:layout_constraintEnd_toStartOf="@+id/textView4" app:layout_constraintHorizontal_bias="0.078"
            app:layout_constraintBottom_toTopOf="@+id/view5"
            android:layout_marginBottom="15dp" tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"/>
    <ImageButton
            android:background="#00ff0000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/add3" android:id="@+id/btn_regist"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.498"
            tools:ignore="SpeakableTextPresentCheck" app:layout_constraintBottom_toBottomOf="@+id/view5"
            android:layout_marginBottom="25dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>